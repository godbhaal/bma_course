<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jon.com.bma_dia2">
    <!--android:label="@string/app_name"-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/bma_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- Declare which java file is the main activity -->
        <!--<activity android:name=".ButtonActivity">-->
        <activity android:name=".CronoActivity">
        <!--<activity android:name=".InterfaceActivity">-->
            <intent-filter>
                <!-- Define it as the first element to run -->
                <action android:name="android.intent.action.MAIN"/>
                <!-- Allow it to be run in the launcher -->
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
